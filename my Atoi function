// my Atoi function 
// coverts a string to an int



#include <stdio.h>
#include <string.h>
int myAtoi(char*str)
{
  int i=0;
  int res=0;
  for (i=0; str[i] != '\0';i++)
  {
    res= res*10+str[i]-'0';
  }
return res;
}





int main(void) {
  int p;
  char c[256] = "255";
  p=myAtoi(c);
  printf("%d",p);
  return 0;
}
