

/*Convert the flowcharts given for walthrough in assignment 1 and test 1 to C codes,
run them and compare the output with your walkthroughs.*/

#include <stdio.h>
#include <stdlib.h>

int main()

{
    int a,b,c;
    a=9;
    b=10;
    c=1;
    while (a>0&&b>0&&c>=0)
    {
        while (b>0&&c>0)
           {

              if (b>5)
               {
                    b=b-2;
                    c=c+1;
                }
              else
               {
                  c=c-2;
                  b=0;
               }
              printf("B=%d\n",b);
              printf("C=%d\n",c);
          }

           a=a-3;
           b=a;
           c=0;
           printf("A=%d\n",a);
    }
     printf("program Complete");



}

